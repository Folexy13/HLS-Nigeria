{
  "builds": [
    {
      "src": "./HLSBACK/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "HLSBACK/wsgi.py"
    }
  ]
}
